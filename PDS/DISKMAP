EXFORMAT LTR   R2,R2               ANY EXTENT.Q
         BZR   R14                 RETURN IMM IF NOT
         ST    R14,EXSAVE
         STM   R2,R3,EXSAVE+4
         SPACE 1
*    IF EAV, DISK ADDR'S IN THE EXTENT TABLE ARE IN NORMALIZED FORM @LV
*    (CCCCCCCH), ELSE THEY'RE IN "OLD" FORM (CCCCHHHH)             @LV
         TM    FLAGS,EAVIND        IS THIS AN EAV ?                @LV
         BZ    EXFMTOLD            NO, BRANCH TO DO THE 'OLD' WAY  @LV
         ICM   R0,B'1111',EXTLCCHH START CCCCCCCH                  @LV
         SRL   R0,4                LEAVE JUST CCCCCCC              @LV
         BAS   R14,CONVERT         TO PRINTED-DECIMAL              @LV
         MVC   EXLFCCD,CONVEDIT+5  TO PRINT LINE                   @LV
         IC    R0,EXTLCCHH+3       'CH'                            @LV
         N     R0,=X'0000000F'     JUST THE 'H' (HEAD #)           @LV
         BAS   R14,CONVERT         TO PRINTED-DECIMAL              @LV
         MVC   EXLFHHD,CONVEDIT+10 TO PRINT LINE                   @LV
         ICM   R0,B'1111',EXTHCCHH ENDING CCCCCCCH                 @LV
         SRL   R0,4                LEAVE JUST CCCCCCC              @LV
         BAS   R14,CONVERT         TO PRINTED-DECIMAL              @LV
         MVC   EXLLCCD,CONVEDIT+5  TO PRINT LINE                   @LV
         IC    R0,EXTHCCHH+3       'CH'                            @LV
         N     R0,=X'0000000F'     JUST THE 'H' (HEAD #)           @LV
         BAS   R14,CONVERT         TO PRINTED-DECIMAL              @LV
         MVC   EXLLHHD,CONVEDIT+10 TO PRINT LINE                   @LV
         UNPK  TRAREA,EXTLCCHH(8)  TO QUASI PRINTED-HEX
         TR    TRAREA,TRTABLE      NOW 'REAL' PRINTED-HEX          @LV
         MVC   EXLFCCE(7),TRAREA   CCCCCCC
         MVI   EXLFCCE+7,C':'      .
         MVC   EXLFCCE+8(1),TRAREA+7  H                            @LV
         SPACE 1                                                   @LV
         UNPK  TRAREA,EXTHCCHH(8)  TO QUASI PRINTED-HEX            @LV
         TR    TRAREA,TRTABLE      NOW 'REAL' PRINTED-HEX          @LV
         MVC   EXLLCCE(7),TRAREA   CCCCCCC
         MVI   EXLLCCE+7,C':'      .                               @LV
         MVC   EXLLCCE+8(1),TRAREA+7  H                            @LV
         B     EXFMCONT                                            @LV
EXFMTOLD DS    0H                                                  @LV
         SLR   R0,R0
         ICM   R0,B'0011',EXTLCCHH  START CC (CYL)
         BAS   R14,CONVERT         TO PRINTED-DECIMAL
         MVC   EXLFCCD,CONVEDIT+5
         ICM   R0,B'0011',EXTLCCHH+2  START HH (HEAD)
         BAS   R14,CONVERT         TO PRINTED-DECIMAL
         MVC   EXLFHHD,CONVEDIT+10
         ICM   R0,B'0011',EXTHCCHH  END CC (CYL)
         BAS   R14,CONVERT         TO PRINTED-DECIMAL
         MVC   EXLLCCD,CONVEDIT+5
         ICM   R0,B'0011',EXTHCCHH+2  END HH (HEAD)
         BAS   R14,CONVERT         TO PRINTED-DECIMAL
         MVC   EXLLHHD,CONVEDIT+10
         UNPK  TRAREA,EXTLCCHH(8)
         TR    TRAREA,TRTABLE
         MVC   EXLFCC,TRAREA
         MVI   EXLFPRD,C'.'
         MVC   EXLFHH,TRAREA+4
         SPACE 1
         UNPK  TRAREA,EXTHCCHH(8)
         TR    TRAREA,TRTABLE
         MVC   EXLLCC,TRAREA
         MVI   EXLLPRD,C'.'
         MVC   EXLLHH,TRAREA+4
EXFMCONT DS    0H                                                  @LV
         SPACE 1
         CLC   EXTHITT,TRKSPVOL    HI TRK > TRKS/VOL ?             @LV
         BNL   MAPINVAL            YES, BRANCH TO MARK IT INVALID  @LV
         CLC   EXTLOTT,EXTHITT     LOW TRK > HIGH TRK ?            @LV
         BH    MAPINVAL            YES, BRANCH TO MARK IT INVALID  @LV
EXRETURN DS    0H                  RETURN BACK FROM 'MAPINVAL'     @LV
         L     R0,EXTLOTT                                   GDL-94
*        N     R0,=X'0000FFFF'  ZERO OUT HIGH ORDER BYTES   GDL-94
         BAS   R14,CONVERT
         MVC   EXLFTT,CONVEDIT+5                            GDL-94 @LV
         SPACE 1
         L     R0,EXTHITT                                   GDL-94
*        N     R0,=X'0000FFFF'  ZERO OUT HIGH ORDER BYTES   GDL-94
         BAS   R14,CONVERT
         MVC   EXLLTT,CONVEDIT+5                            GDL-94 @LV
         SPACE 1
         L     R0,EXTNOTRK                                  GDL-94
         BAS   R14,CONVERT
         MVC   EXLNOTRK,CONVEDIT+5                          GDL-94 @LV
         SPACE 1
         CLI   EXPFLAG,0
         BNE   EXPTYPE
         L     R1,EXTIDPTR
         MVC   EXLDSNAM,0(R1)
         SPACE 1
EXPTYPE  CLI   EXTTYPE,X'01'       IS THIS A DATA EXTENT.Q
         BNE   EXPNDATA            NO, GO CHECK OTHERS
         CLI   EXPFLAG,0           ARE WE MAPPING.Q
         BE    EXPEXTPR            YES, DON'T CLUTTER UP
         MVC   EXLTYPE,=C'PRIM'    ELSE MOVE IN EXTENT TYPE
         B     EXPEXTPR
         SPACE 1
EXPNDATA TM    EXTTYPE,X'F0'       VOL LABEL, OR TYPE 4, 5, OR 6
         BO    EXPCKFMX            BR IF YES
         CLI   EXTTYPE,X'02'       ISAM OVERFLOW.Q
         MVC   EXLTYPE,=C'OVFL'
         BE    EXPEXTPR
         SPACE 1
         CLI   EXTTYPE,X'04'       ISAM INDEX.Q
         MVC   EXLTYPE,=C'INDX'
         BE    EXPEXTPR
         SPACE 1
         CLI   EXTTYPE,X'40'       USER LABEL TRACK.Q
         MVC   EXLTYPE,=C'ULBL'
         BE    EXPEXTPR
         SPACE 1
         CLI   EXTTYPE,X'81'       CYLS OF DATA ON CYL BDRY.Q
         MVC   EXLTYPE,=C'CYLB'
         BE    EXPEXTPR
         SPACE 1
         CLI   EXTTYPE,X'80'       SHARING AN EXTENT.Q
         MVC   EXLTYPE,=C'SHRD'
         BNE   EXPCKFMX
         CLI   EXPFLAG,0           ARE WE MAPPING.Q
         BNE   EXPEXTPR            NO, DON'T ENCLOSE IN PARENS
         MVI   EXLLPARN,C'('
         MVI   EXLRPARN,C')'
         B     EXPEXTPR
         SPACE 1
EXPCKFMX CLI   EXTTYPE,C'5'        FREE EXTENT.Q
         BE    EXPPRLIN
         SPACE 1
         CLI   EXTTYPE,C'4'        VTOC EXTENT.Q
         MVC   EXLTYPE,=C'VTOC'
         BE    EXPEXTPR
         SPACE 1
         CLI   EXTTYPE,X'FF'       VOLUME LABEL.Q
         MVC   EXLTYPE,=C'VLBL'
         BE    EXPPRLIN
         SPACE 1
         CLI   EXTTYPE,C'6'        SHARED EXTENT.Q
         MVC   EXLTYPE,=C'*UN*'
         BNE   EXPEXTPR
         SPACE 1
         MVC   EXLTYPE,=C'SPLT'
         SR    R0,R0
         IC    R0,EXTSEQNO
         BAS   R14,CONVERT
         MVC   EXLSPLIT(3),CONVEDIT+9
         B     EXPPRLIN
         SPACE 1
EXPEXTPR SR    R0,R0
         IC    R0,EXTSEQNO
         AH    R0,=H'1'
         BAS   R14,CONVERT
********************************** START *********** SMA 3/88  CHANGED
         MVC   EXLSEQNO,CONVEDIT+9
**********************************  END  *********** SMA 3/88  CHANGED
         SPACE 1
