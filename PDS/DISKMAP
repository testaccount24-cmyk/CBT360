         EJECT
         SPACE 3
*      THIS ROUTINE ADDED 03/82 BY D. KOWNACKI FOR INDEXED VTOCS
*
*       IF INDEXED VTOC THIS ROUTINE WILL BUILD A FORMAT 5 DSCB.
*        FMT5AD CONTAINS THE ADDRES OF THE FORMAT 5 DSCB BUFFER.
*        IT CONTAINS THE LAST FORMAT 5 DSCB READ OR BUILT. THE
*        FMT 5 BUFFER IS ZERO IF THIS IS THE 1ST CALL.
*        THE BUILT FMT5 WILL THEN BE PROCESSED AS BEFORE.
*        IF THE FMT 5 DSCB BUFFER RETURNED INDICATES MORE EXTENTS
*         THIS ROUTINE WILL BE CALLED AGAIN TO OBTAIN THE
*         NEXT FMT 5 DSCB AND SO ON.
*
*        USING DS5,R8                                         GDL-94
         USING DS4,R6
*
INDEXCHK ST    R14,INDXSAVE
         L     R5,UCBADDR               PUT UCBADDR IN REG 5
         CVAFTST UCB=(R5)               TEST VTOC
         ST    R15,CVFTSTRC             SAVE CVAFTST RETURN CODE
         LTR   R15,R15                  TEST RETURN CODE
         BNZ   INDXED                    GO PROCESS           GDL-94
         BAS   R14,ABDUMP        INDEX VTOC SUPPORT REQUIRED  GDL-94
*        BE    INDXEND                   GO PROCESS THE FMT5  GDL-94
         SPACE 1
*        ASSUME INDEXED VTOC AND GET THE NO OF AVAILABLE FORMAT 1'S
         SPACE 1
INDXED   ST    R5,CVPL2+12           STORE UCBADDR FOR MACRO
         CLC   CVFTSTRC,=F'8'        CVAFTST RC=8 ?
*                                    (WHICH MEANS VOLUME IS IXVTOC)
         BNE   NOTINDX               BRANCH IF *NOT* IXVTOC
         L     R2,DEBADDR
         CVAFDSM  MF=(E,CVPL2),BRANCH=NO
         SPACE 1
******** MVC   DS4DSREC(2),FMT0ADDR+2   STORE AVAIL FMT0 IN FMT4 FIELD
         MVC   AVALDSCB,FMT0ADDR        SAVE *FULL*-WORD FREE-DSCB CNT
         SPACE 1
NOTINDX  DS    0H
*        ASSUME INDEXED VTOC ---     NOT APPLICABLE        GDL-94
         MVC   DT2X7HDR,ZEROES       ZERO OUT HEADER       GDL-94
         MVC   DT2X7EYE,=C'ICVEDT02' INIT EYE CATCHER      GDL-94
         MVC   DT2X7LEN,DT2LEN       TOTAL AREA LENGTH
         MVC   DT2X7LEV,DT2LEV       CONTROL BLOCK LEVEL
         MVC   DT2ENTRY(4),ZEROES    ZERO RTA START ADDR   GDL-94
         MVC   DT2ENTNM,NUMENT    INIT NUMBER OF ENTRIES   GDL-94
         SPACE 1
         ST    R5,CVUCB              STORE UCBADDR FOR MACRO
*                                                          GDL-94
*                                                          GDL-94
MOREINDX DS    0H
         L     R2,DEBADDR
         CVAFDSM MF=(E,CVPL),BRANCH=NO   GET EXTENTS FROM CVPL GDL-94
         SPACE 1
*        TM    CVFL1,CV1IVT                          GDL-94
*        BZ    INDXEND                               GDL-94
         LTR   R15,R15                     IS THERE AN ERROR?
         BZ    BUILDF5                      NO       GDL-94
         C     R15,=F'4'                   TEST THE RETURN CODE
         BE    TSTSTAT
         BAS   R14,ABDUMP                  UNEXPECTED ERROR (ABEND)
TSTSTAT  CLI   CVSTAT,STAT032              END OF DATA
         BE    INDXEND                               GDL-94
*  INSTEAD OF ABEND'ING, OUTPUT A MSG AND ATTEMPT TO CONTINUE      KBC
         LA    R2,CVSTAT                   ERROR CODE              KBC
         BAS   R14,CONVERT1                TO DECIMAL EBCDIC       KBC
         MVC   DSMSTAT,CONVERTD            TO PRINT LINE           KBC
         L     R15,CHGTO24M            LOAD ADDR OF 'PUT224' WITH
         BSM   0,R15                   CHANGE TO 24-BIT MODE IN ORDER
*                                      TO DO I/O
CHGTO24M DC    0F'0',AL1(00),AL3(PUTERM)   SWITCH TO 24-BIT TO DO I/O
PUTERM   DS    0H
         PUT   PRINTDCB,DSMERMSG           OUTPUT ERROR MSG        KBC
         L     R15,CHGTO31N             LOAD ADDR OF 'PUT231' WITH
         BSM   0,R15                   CHANGE TO 31-BIT MODE IN ORDER
*                                      TO ADDRESS GETCORE STORAGE
CHGTO31N DC    0F'0',A(X'80000000'+PUT31BK)  SWITCH BACK TO 31-BIT
PUT31BK  DS    0H
         B     INDXEND                     "PUNT" INSTEAD OF ABEND KBC
         BAS   R14,ABDUMP                  UNEXPECTED ERROR (ABEND)
         SPACE 3
*        BUILD THE FORMAT 5 ***NO LONGER DONE***     GDL-94
*    BUT NEED TO PROCESS CURRENT GROUP OF EXTENTS AND GET MORE  GDL-94
         SPACE 2
BUILDF5  DS    0H
*        MVC   DS5KEYID,FM5ID                        GDL-94
*        MVC   DS5AVEXT(L'DS5AVEXT+L'DS5EXTAV),EXTS  GDL-94
*                                                    GDL-94
*        MVI   DS5FMTID,C'5'                         GDL-94
*        MVC   DS5MAVET,EXTS+L'DS5AVEXT+L'DS5EXTAV   GDL-94
*        NC    EXTS+L'EXTS-L'DS5AVEXT(5),EXTS+L'EXTS-L'DS5AVEXT GDL-94
*                                                    GDL-94
*        BZ    INDXEND          BRANCH IF YES        GDL-94
         BAS   R14,F56HOOK
******** MVC   DT2ENTRY(4),DT2END-4 SETUP FOR NEXT TIME THRU       @LVD
*                           PER APAR OY66889                   GDL-94
         L     R14,=A(DT2END-4)   ADDRESS THE END-4                @LV
         MVC   DT2ENTRY(4),0(R14)  SETUP FOR NEXT TIME THRU        @LV
*                           PER APAR OY66889                       @LV
*        CLC   DT2ENTRY(4),=X'0000C3A5'  LAST CYL 3390-3 KOHLS-02  KBCD
*        BE    INDXEND                                   KOHLS-02  KBCD
*        CLC   DT2ENTRY(4),=X'00024AEF'  LAST CYL 3390-9 KOHLS-02  KBCD
*        BE    INDXEND                                   KOHLS-02  KBCD
         B     MOREINDX
*                         ALL EXTENTS PROCESSED EXIT.
INDXEND  DS    0H
         L     R14,INDXSAVE
         BR    R14
         EJECT
         SPACE 3
HOOKUP   DS    0H
         TM    PARMOPT2,FREEOPT
         BO    HOOKCONT
         TM    PARMOPT1,MAPOPT+EXTNTOPT+DIRFLAG
         BZR   R14
HOOKCONT DS    0H
         ST    R14,HOSAVE
         STM   R3,R5,HOSAVE+4
         LA    R4,EXTNTLEN
         BAS   R14,GETCORE
         USING EXTENT,R3
         MVC   EXTENT(EXTNTLEN),0(R2)                              @LV
*   IF THIS IS AN EAV, "NORMALIZE" THE EXTENT'S DISK ADDRESSES     @LV
*   SUCH/SO THAT COMPARES AND SUCH WORK "AS BEFORE"                @LV
*   UNLESS THE 'NONORMIT' FLAG IS ON (FROM "F5" EXTENT-CONVERT)    @LV
         TM    FLAGS,EAVIND        IS THIS AN EAV ?                @LV
         BZ    HOOKREG             NO, BRANCH (LEAVE CCHH "AS IS") @LV
         TM    FLAGS,NONORMIT      IS THE NO-NORMALIZE FLAG ON ?   @LV
         BO    HOOKREG             YES, BRANCH TO NOT NORMALIZE    @LV
         TRKADDR NORMALIZE,EXTLCCHH,REG=R14  NORMALIZE START ADDR  @LV
         ST    R14,EXTLCCHH        STORE NORMALIZED CCCCCCCH BACK  @LV
         TRKADDR NORMALIZE,EXTHCCHH,REG=R14  NORMALIZE ENDING ADDR @LV
         ST    R14,EXTHCCHH        STORE NORMALIZED CCCCCCCH BACK  @LV
HOOKREG  DS    0H                                                  @LV
         L     R2,HOOKSTRT
         LA    R4,HOOKSTRT
HOOKLOOK CLC   EXTLCCHH,EXTLCCHH-EXTENT(R2)
         BL    HOOKDONE
         LR    R4,R2
         L     R2,0(R2)
         B     HOOKLOOK
         SPACE 1
HOOKDONE ST    R2,EXTPTR
         ST    R3,EXTPTR-EXTENT(R4)
         LR    R15,R3
         LM    R3,R5,HOSAVE+4
         L     R14,HOSAVE
         BR    R14
         SPACE 1
         DROP  R3
         EJECT ,
         SPACE 3
CCHHCONV DS    0H                                                  @LV
         TM    FLAGS,EAVIND        IS THIS AN EAV ?                @LV
         BZ    DOOLDCNV            NO, DO IT THE OLD WAY           @LV
         TRKADDR ABSTOREL,(R2),REG=(R0,R1)  CONVERT TO RTA         @LV
         BR    R15                 RETURN TO CALLER                @LV
DOOLDCNV DS    0H
         SR    R0,R0                                               @LV
         IC    R0,0(R2)
         MH    R0,DEVCONV1
         SR    R1,R1
         IC    R1,1(R2)
         MH    R1,DEVCONV2
         AR    R0,R1
         SR    R1,R1
         IC    R1,2(R2)
         MH    R1,DEVCONV3
         AR    R0,R1
         SR    R1,R1
         IC    R1,3(R2)
         MH    R1,DEVCONV4
         AR    R0,R1
         BR    R15
         SPACE 4
TTCONVRT DS    0H                                                 @LV2
*   IF EAV, USE TRKADDR TO CONVERT RELATIVE TO ABSOLUTE, AND THEN  @LV
*   NORMALIZE THE RESULT IN FIELD "CCHH"                           @LV
         TM    FLAGS,EAVIND        IS THIS AN EAV ?                @LV
         BZ    TTCNVOLD            NO, DO IT THE "OLD" WAY         @LV
         SR    R0,R0                                              @LV2M
         ST    R0,CCHH                                            @LV2M
         TRKADDR RELTOABS,CONVWORK,REG=R0   CONVERT TO CCCCCCCH    @LV
         TRKADDR NORMALIZE,CONVWORK,REG=R0  NORMALIZE IT           @LV
         ST    R0,CCHH             SAVE RESULTS IN "RETURN" AREA   @LV
         BR    R14                 BACK TO CALLER                  @LV
TTCNVOLD DS    0H
         SR    R0,R0                                              @LV2
         ST    R0,CCHH                                            @LV2
         LH    R15,DEVCONV1
         LTR   R15,R15
         BZ    TTCONVR2
         DR    R0,R15
         STC   R1,CCHH
         LR    R1,R0
         SR    R0,R0
TTCONVR2 LH    R15,DEVCONV2
         LTR   R15,R15
         BZ    TTCONVR3
         DR    R0,R15
         STC   R1,CCHH+1
         LR    R1,R0
         SR    R0,R0
TTCONVR3 LH    R15,DEVCONV3
         LTR   R15,R15
         BZ    TTCONVR4
         DR    R0,R15
         STC   R1,CCHH+2
         STC   R0,CCHH+3
         BR    R14
         SPACE 1
TTCONVR4 STC   R1,CCHH+3
         BR    R14
         EJECT
         SPACE 3
CONVERT1 LA    R3,1
         B     CONVCOM
         SPACE 1
CONVERT2 LA    R3,2
         B     CONVCOM
         SPACE 1
CONVERT3 LA    R3,3
         B     CONVCOM
         SPACE 1
CONVERT4 LA    R3,4
         SPACE 1
CONVCOM  SR    R0,R0
CONV1MOR SLL   R0,8
         IC    R0,0(R2)
         LA    R2,1(R2)
         BCT   R3,CONV1MOR
         SPACE 1
CONVERT  LPR   R0,R0
         CVD   R0,CONVDEC
         UNPK  CONVUNPK,CONVDEC
         OI    CONVUNPK+11,X'F0'
         MVC   CONVEDIT,=X'402020202020202020202120'
         ED    CONVEDIT,CONVDEC+2
         MVC   CONVERTD,CONVEDIT+1
CONVCLI  CLI   CONVERTD,C' '
         BNER  R14
         MVC   CONVERTD,CONVERTD+1
         B     CONVCLI
         SPACE 1
         EJECT
         SPACE 3
DATEDIT  ST    R14,DASAVE
         STM   R2,R5,DASAVE+4
         XC    DATEWORD,DATEWORD
         LTR   R2,R2
         BZ    DATGET
         SR    R1,R1
         IC    R1,0(R2)
         MH    R1,=H'1000'
         MVC   DATEWORD,1(R2)
         AH    R1,DATEWORD
         CVD   R1,DATEWORD
DATGETRT OI    DATEWORD+7,X'0F'
         L     R2,DATEWORD+4
         SR    R0,R0
         D     R0,=F'1000'
         LA    R15,DATNORMY
         LTR   R14,R1
         BZ    DATCOMP
         N     R14,=F'3'
         BM    DATCOMP
         LA    R15,DATLEAPY
         SPACE 1
DATCOMP  LTR   R0,R0
         BZ    DAJULIAN
         CH    R0,0(R15)
         BH    DAJULIAN
         TM    PARMOPT2,JDATEOPT
         BO    DAJULIAN
         LA    R3,1
         SR    R14,R14
         SPACE 1
DATICMON IC    R14,1(R3,R15)
         CR    R0,R14
         BNH   DATMONOK
         SR    R0,R14
         LA    R3,1(R3)
         B     DATICMON
         SPACE 1
DATMONOK MH    R3,=H'1000'
         AR    R3,R0
         MH    R3,=H'1000'
         AR    R3,R1
         CVD   R3,DATEWORD
         MVC   DATEWORD(5),DATEWORD+3
         UNPK  DATEWORD(8),DATEWORD(5)
         OI    DATEWORD+7,X'F0'
         MVI   DATEWORD+2,C'/'
         MVI   DATEWORD+5,C'/'
         SPACE 3
DATRETRN LM    R2,R5,DASAVE+4
         MVC   0(8,R3),DATEWORD
         L     R14,DASAVE
         BR    R14
         SPACE 2
DAJULIAN ST    R2,DATEWORD
         UNPK  DATEWORD+2(5),DATEWORD+1(3)
         MVC   DATEWORD+1(2),DATEWORD+2
         MVI   DATEWORD,C' '
         MVI   DATEWORD+3,C'.'
         MVI   DATEWORD+7,C' '
         B     DATRETRN
         SPACE 2
DATGET   ST    R1,DATEWORD+4
         LTR   R1,R1
         BNZ   DATGOT
         TIME  DEC
         ST    R1,DATEWORD+4
DATGOT   CVB   R1,DATEWORD
         B     DATGETRT
         SPACE 2
         EJECT
         SPACE 3
DUMPDSCB ST    R14,DUSAVE
         LR    R4,R2
         LA    R2,3
         BAS   R14,PRLKAHED
         LR    R2,R4
         MVC   BUF+8(17),=C'BLK 1234.5678.90:'
         UNPK  TRAREA,3(8,R3)
         TR    TRAREA,TRTABLE
         MVC   BUF+12(4),TRAREA
         MVC   BUF+17(4),TRAREA+4
         MVC   BUF+22(2),TRAREA+8
         LA    R3,0(R2)
         LA    R4,BUF+35
         LA    R5,44
         BAS   R14,DUMPLINE
         MVI   BUF,SPACE1AP
         BAS   R14,PRINT
         MVC   BUF+8(13),=C'FORMAT N DSCB'
         MVC   BUF+15(1),44(R2)
         OI    BUF+15,X'F0'
         LA    R3,44(R2)
         LA    R4,BUF+26
         LA    R5,48
         BAS   R14,DUMPLINE
         MVI   BUF,SPACE1AP
         BAS   R14,PRINT
         LA    R3,92(R2)
         LA    R4,BUF+26
         LA    R5,48
         BAS   R14,DUMPLINE
         MVI   BUF,SPACE2AP
         BAS   R14,PRINT
         L     R14,DUSAVE
         BR    R14
         EJECT ,
         SPACE 3
DUMPLINE LTR   R5,R5
         BNPR  R14
         B     DUMPNEXT
DUMPWORD BCT   R1,DUMPBYTE
         MVI   0(R4),C' '
         LA    R4,1(R4)
DUMPNEXT LA    R1,4
DUMPBYTE UNPK  TRAREA(3),0(2,R3)
         TR    TRAREA(2),TRTABLE
         MVC   0(2,R4),TRAREA
         LA    R3,1(R3)
         LA    R4,2(R4)
         BCT   R5,DUMPWORD
         BR    R14
         EJECT
         SPACE 3
GETCORE  A     R4,=F'3'
         N     R4,=F'-4'
GETAGAIN L     R0,CORLEFT
         SR    R0,R4
         BM    GETMAIN
         ST    R0,CORLEFT
         L     R3,CORNEXT@
         LR    R1,R4               PUT 'GET' LENGTH INTO R1        KBC
         LA    R0,K256             256 IS MAX OF 'XC' INSTRUCTION  KBC
         CR    R1,R0               'GET' LENGTH > 256 ?            KBC
         BNH   GETLE256            NO, BRANCH                      KBC
         LR    R1,R0               SET MAX LENGTH TO 256 FOR XC    KBC
GETLE256 DS    0H                                                  KBC
         BCTR  R1,0                LESS ONE FOR XC                 KBC
         EX    R1,GETCLEAR         CLEAR (ZERO) THE STORAGE        KBC
         LR    R0,R3
         AR    R0,R4
         ST    R0,CORNEXT@
         BR    R14
         SPACE 1
GETCLEAR XC    0(0,R3),0(R3)       ZERO STORAGE (SUBJECT OF 'EX')  KBC
         SPACE 3
GETMAIN  L     R3,CURBLK@
         GETMAIN RC,LV=2048,LOC=31     GET 31-BIT STORAGE (VSCR)
         BXH   R15,R15,ERRNOCOR
         ST    R1,CURBLK@              SAVE RETURNED-STORAGE ADDR
         SPACE 1
         L     R1,CURBLK@
         ST    R1,0(R3)
         SR    R0,R0
         ST    R0,0(R1)
         LA    R1,4(R1)
         ST    R1,CORNEXT@
         MVC   CORLEFT,=A(2044)
         B     GETAGAIN
         EJECT ,
         SPACE 3
FREECORE L     R2,CORFRST@
FREELOOP LTR   R3,R2
         BZ    FREEDONE
         L     R2,0(R2)
         FREEMAIN RU,LV=2048,A=(3)
         B     FREELOOP
         SPACE 1
FREEDONE SR    R0,R0
         ST    R0,CORLEFT
         ST    R0,CORFRST@
         LA    R0,CORFRST@
         ST    R0,CURBLK@
         BR    R14
         EJECT
         SPACE 3
PRINT    ST    R14,PRSAVE
         STM   R2,R5,PRSAVE+4
         LH    R0,CURLINES
         CH    R0,MAXLINES
         BL    PRNOPAGE
         L     R15,CHGTO24D            LOAD ADDR OF 'PUT224' WITH
         BSM   0,R15                   CHANGE TO 24-BIT MODE IN ORDER
*                                      TO DO I/O
CHGTO24D DC    0F'0',AL1(00),AL3(PUT224)   SWITCH TO 24-BIT TO DO I/O
PUT224   DS    0H
         PUT   PRINTDCB,NEWPAGE
         L     R15,CHGTO31E             LOAD ADDR OF 'PUT231' WITH
         BSM   0,R15                   CHANGE TO 31-BIT MODE IN ORDER
*                                      TO ADDRESS GETCORE STORAGE
CHGTO31E DC    0F'0',A(X'80000000'+PUT231)   SWITCH BACK TO 31-BIT
PUT231   DS    0H
         SR    R0,R0
         STH   R0,CURLINES
PRNOPAGE LTR   R0,R0
         BNZ   PRINTBUF
         LH    R0,CURPAGES
         AH    R0,=H'1'
         STH   R0,CURPAGES
         CH    R0,MAXPAGES
         BNH   PRTPGCNV
         SR    R0,R0
         STH   R0,CURPAGES
PRTPGCNV BAS   R14,CONVERT
         MVC   PAGENO,CONVERTD
         L     R15,CHGTO24E            LOAD ADDR OF 'PUT224' WITH
         BSM   0,R15                   CHANGE TO 24-BIT MODE IN ORDER
*                                      TO DO I/O
CHGTO24E DC    0F'0',AL1(00),AL3(PUT324)   SWITCH TO 24-BIT TO DO I/O
PUT324   DS    0H
         PUT   PRINTDCB,HEADERCC
         L     R15,CHGTO31F             LOAD ADDR OF 'PUT331' WITH
         BSM   0,R15                   CHANGE TO 31-BIT MODE IN ORDER
*                                      TO ADDRESS GETCORE STORAGE
CHGTO31F DC    0F'0',A(X'80000000'+PUT331)   SWITCH BACK TO 31-BIT
PUT331   DS    0H
         MVC   CURLINES,=H'3'
         L     R2,BUFPTR
PRNXTBUF LH    R0,CURLINES
         LTR   R2,R2
         BZ    PRINTBUF
         AH    R0,4(R2)
         STH   R0,CURLINES
         LA    R3,6(R2)
         L     R15,CHGTO24G            LOAD ADDR OF 'PUT524' WITH
         BSM   0,R15                   CHANGE TO 24-BIT MODE IN ORDER
*                                      TO DO I/O
CHGTO24G DC    0F'0',AL1(00),AL3(PUT524)   SWITCH TO 24-BIT TO DO I/O
PUT524   DS    0H
         PUT   PRINTDCB,(3)
         L     R15,CHGTO31H             LOAD ADDR OF 'PUT531' WITH
         BSM   0,R15                   CHANGE TO 31-BIT MODE IN ORDER
*                                      TO ADDRESS GETCORE STORAGE
CHGTO31H DC    0F'0',A(X'80000000'+PUT531)   SWITCH BACK TO 31-BIT
PUT531   DS    0H
         L     R2,0(R2)
         B     PRNXTBUF
         EJECT
         SPACE 3
PRINTBUF TM    BUF,X'80'
         BZ    PRUPLINE
         SR    R0,R0
         B     PRDOPUT
PRUPLINE SR    R14,R14
         IC    R14,BUF
         SRL   R14,3
         AR    R0,R14
PRDOPUT  STH   R0,CURLINES
         L     R15,CHGTO24H            LOAD ADDR OF 'PUT624' WITH
         BSM   0,R15                   CHANGE TO 24-BIT MODE IN ORDER
*                                      TO DO I/O
CHGTO24H DC    0F'0',AL1(00),AL3(PUT624)   SWITCH TO 24-BIT TO DO I/O
PUT624   DS    0H
         PUT   PRINTDCB,BUF
         L     R15,CHGTO31I             LOAD ADDR OF 'PUT631' WITH
         BSM   0,R15                   CHANGE TO 31-BIT MODE IN ORDER
*                                      TO ADDRESS GETCORE STORAGE
CHGTO31I DC    0F'0',A(X'80000000'+PUT631)   SWITCH BACK TO 31-BIT
PUT631   DS    0H
         TM    BUF,X'02'
         BO    PRETURN
         MVI   BUF+1,C' '
         MVC   BUF+2(131),BUF+1
         SPACE 1
PRETURN  LM    R2,R5,PRSAVE+4
         L     R14,PRSAVE
         BR    R14
         SPACE 4
PRLKAHED AH    R2,CURLINES
         CH    R2,MAXLINES
         BNHR  R14
         MVC   CURLINES,=H'999'
         BR    R14
         EJECT
         SPACE 1
RETURN   CLOSE (PRINTDCB)
         SPACE 1
         TM    DCBBUFCB+3+(PRINTDCB-IHADCB),X'01'
         BO    RETQUIT
         FREEPOOL PRINTDCB
         SPACE 1
RETQUIT  CLI   HDNGLIST,X'FF'
         BNE   RETSAVER
         L     R1,HDNGLIST
         LH    R2,0(R1)
         LH    R0,CURPAGES
         AH    R0,=H'1'
         BAS   R14,CONVERT
         LA    R3,CONVEDIT+12
         SR    R3,R2
         BCTR  R2,0
         STC   R2,*+5
         MVC   2(*-*,R1),0(R3)
         SPACE 1
RETSAVER DS    0H
         L     R13,SAVEAREA+4
         L     R15,RETCODE
         L     R14,12(R13)
         LM    R0,R12,20(R13)
         MVI   12(R13),X'FF'
         SPM   R14
         BR    R14
         SPACE 3
ABDUMP   OI    ABCODE,X'80'        SET DUMP FLAG
         SPACE 1
ABEND    L     R1,ABCODE           PICK UP ABEND CODE
         ABEND (1)
         EJECT ,
HEADWVER DC    CL133'                         DISKMAP VERSION:&SYSDATC'
*                                                                 KBC
DSMERMSG DS    0CL133
         DC    C'  ***ERROR*** -- UNEXPECTED ERROR FROM CVAFDSM'  KBC
         DC    C' -- CVSTAT='                                     KBC
DSMSTAT  DS    CL3                                                KBC
         DC    C' -- ATTEMPTING TO CONTINUE ...'                  KBC
         DC    CL(133-(*-DSMERMSG))' '                            KBC
NEWPAGE  DC    AL1(EJECTIM)
HEADERCC DC    AL1(SPACE3AP)
VOLSERNO DS    CL6                 CURRENT VOLUME SERIAL
         ORG   VOLSERNO
HEADER DS 0CL95
       DC C'DIRECT-ACCESS VOLUME LISTING UTILITY....            '
       DC C'   PAS. VERSION--'
       DC C'&SYSDATE'   8 CHARS
       DC CL28' '                                                  KBC
