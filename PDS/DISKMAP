         USING DT2ARENT,R2                               GDL-94
*                                                        GDL-94
F56HOOK  ST    R14,F5SAVE
         LA    R2,DT2ENTRY        POINT TO FIRST ENTRY   GDL-94
         L     R3,DT2ENTNM        LOAD NUMBER OF ENTRIES GDL-94
F5LOOP   CLC   DT2EXT,ZEROES      ZERO ENTRY             GDL-94
         BE    F5EXIT                 END OF EXTENTS     GDL-94
*        CLC   DT2ENTRY(4),=X'0000C3A5'  LAST CYL 3390-3 KOHLS-02 KBCD
*        BE    F5EXIT                                    KOHLS-02 KBCD
*        CLC   DT2ENTRY(4),=X'00024AEF'  LAST CYL 3390-9 KOHLS-02 KBCD
*        BE    F5EXIT                                    KOHLS-02 KBCD
         BAS   R14,F5EXTCNV
         LA    R2,8(R2)           POINT TO NEXT ENTRY    GDL-94
         BCT   R3,F5LOOP
*        LA    R2,DS5MAVET                               GDL-94
*        LA    R3,18                                     GDL-94
*        BAS   R14,F5EXTCNV                              GDL-94
*        LA    R2,5(R2)                                  GDL-94
*        BCT   R3,*-8                                    GDL-94
F5EXIT   L     R14,F5SAVE                                GDL-94
         BR    R14
         SPACE 1
F5EXTCNV ST    R14,F5SAVE+4
         STM   R2,R5,F5SAVE+8                            GDL-94
         L     R1,DT2RTAST           GET BEGINING TRACK  GDL-94
         ST    R1,HOOKLOTT           STORE IN TABLE      GDL-94
         LR    R3,R1                 SAVE IN R3          GDL-94
         BAS   R14,TTCONVRT          CONVERT TO CCHH     GDL-94
*   NOTE THAT, FOR EAV, TTCONVRT RETURNS NORMALIZED CCCCCCCH       @LV
         MVC   HOOKLCCH,CCHH         SAVE IN TABLE       GDL-94
         L     R1,DT2RTAED           GET ENDING TRACK    GDL-94
         LR    R4,R1                 SAVE IN R4          GDL-94
         BCTR  R1,0                  ADJUST END OF EXT   GDL-94
         ST    R1,HOOKHITT           STORE IN TABLE      GDL-94
         BAS   R14,TTCONVRT          CONVERT TO CCHH     GDL-94
*   NOTE THAT, FOR EAV, TTCONVRT RETURNS NORMALIZED CCCCCCCH       @LV
         MVC   HOOKHCCH,CCHH         SAVE IN TABLE       GDL-94
         SR    R4,R3                 GET NUMBER OF TRKS  GDL-94
         ST    R4,HOOKNTRK           SAVE IN TABLE       GDL-94
         TM    FLAGS,EAVIND        IS THIS AN EAV ?                @LV
         BZ    F5DOOLD             NO, BRANCH TO DO IT THE OLDWAY  @LV
*   NOTE THAT, FOR EAV, WE NEED TO MAINTAIN TWO "BUCKETS"          @LV
*   FOR FREE-SPACE: (1) TOTAL-VOLUME, AND (2) TRK-MANAGED AREA.    @LV
*   CVAF WILL NOT RETURN A FREE-EXTENT THAT CROSSES "THE LINE"     @LV
*   BETWEEN TRK-MANAGED AND CYLINDER-MANAGED.                      @LV
         ICM   R5,B'1111',HOOKHCCH LOAD NORMALIZED END CCCCCCCH    @LV
         SRL   R5,4                SHIFT OUT 'H', LEAVING CCCCCCC  @LV
         ICM   R0,B'1111',HOOKLCCH START CYLINDER (NORMALIZED)     @LV
         SRL   R0,4                SHIFT OUT 'H', LEAVING CCCCCCC  @LV
         SLR   R5,R0               SUBTRACT START CYL              @LV
         LA    R5,1(,R5)           ADJUST COUNT                    @LV
         TM    HOOKLCCH+3,X'0F'    START ON CYL BOUNDARY ?         @LV
         BZ    STRTBDYE            YES                             @LV
         BCTR  R5,0                NO, SUBTRACT 1                  @LV
STRTBDYE EQU   *                                                   @LV
         LH    R0,TRKSPCYL         GET TRACKS PER CYL              @LV
         BCTR  R0,0                BACK UP TO LAST TRK             @LV
         TRKADDR EXTRACTTRK,HOOKHCCH,REG=R1  GET ENDING 'H'        @LV
         CR    R0,R1               END ON CYL BOUNDARY ?           @LV
         BE    ENDBDYE             YES, BRANCH                     @LV
         BCTR  R5,0                NO SUBTRACT 1                   @LV
ENDBDYE  C     R4,F5LARGTT         IS IT THE LARGEST ?             @LV
         BNH   F5AVAILE            NO, BRANCH                      @LV
         ST    R4,F5LARGTT         SAVE NEW LARGEST EXT IN TRKS    @LV
         ST    R5,F5LARGST         NUM CYL IN LARGEST FREE EXT     @LV
F5AVAILE L     R0,AVAILSPC         ADD AVAILABLE TRKS              @LV
         AR    R0,R4               ADD AVAILABLE TRKS              @LV
         ST    R0,AVAILSPC         SAVE UPDATED AVAILABLE TRKS     @LV
         L     R1,AVAILEXT         INCREMENT ...                   @LV
         LA    R1,1(,R1)           COUNT OF AVAIL ...              @LV
         ST    R1,AVAILEXT         EXTENTS                         @LV
         A     R5,AVAILCYL         ADD FULL CYLINDERS              @LV
         ST    R5,AVAILCYL         SAVE UPDATED AVAILABLE CYLS     @LV
*   NOW, DETERMINE IF THE FREE-EXTENT IS IN THE TRK-MANAGED SPACE  @LV
*   (I.E., BELOW "THE LINE"), AND, IF SO, BUCKET TO TRK-MANAGED.   @LV
         ICM   R0,B'1111',HOOKLCCH LOAD NORMALIZED START CCCCCCCH  @LV
         C     R0,THELINE          COMPARE TO "THE LINE" (CCCCCCC0)@LV
         BNL   F5SKPOLD            IF ABOVE THE LINE, BR TO NOT    @LV
*                                  BUCKET TO TRACK-MANAGED         @LV
         SRL   R0,4                SHIFT OUT 'H', LEAVING CCCCCCC  @LV
         ICM   R5,B'1111',HOOKHCCH LOAD NORMALIZED END CCCCCCCH    @LV
         SRL   R5,4                SHIFT OUT 'H', LEAVING CCCCCCC  @LV
         SLR   R5,R0               SUBTRACT START CYL              @LV
         LA    R5,1(,R5)           ADJUST COUNT                    @LV
         TM    HOOKLCCH+3,X'0F'    START ON CYL BOUNDARY ?         @LV
         BZ    STRTBDYT            YES                             @LV
         BCTR  R5,0                NO, SUBTRACT 1                  @LV
STRTBDYT EQU   *                                                   @LV
         LH    R0,TRKSPCYL         GET TRACKS PER CYL              @LV
         BCTR  R0,0                BACK UP TO LAST TRK             @LV
         TRKADDR EXTRACTTRK,HOOKHCCH,REG=R1  GET ENDING 'H'        @LV
         CR    R0,R1               END ON CYL BOUNDARY ?           @LV
         BE    ENDBDYT             YES, BRANCH                     @LV
         BCTR  R5,0                NO SUBTRACT 1                   @LV
ENDBDYT  C     R4,TMLARGTT         IS IT THE LARGEST ?             @LV
         BNH   F5AVAILT            NO, BRANCH                      @LV
         ST    R4,TMLARGTT         SAVE NEW LARGEST EXT IN TRKS    @LV
         ST    R5,TMLARGST         NUM CYL IN LARGEST FREE EXT     @LV
F5AVAILT L     R0,AVALSPTM         AVAILABLE TRKS IN TRK-MANAGED   @LV
         AR    R0,R4               ADD AVAILABLE TRKS              @LV
         ST    R0,AVALSPTM         SAVE UPDATED AVAILABLE TRKS     @LV
         L     R1,AVALEXTM         INCREMENT ...                   @LV
         LA    R1,1(,R1)           COUNT OF AVAIL ...              @LV
         ST    R1,AVALEXTM         EXTENTS                         @LV
         A     R5,AVALCYTM         ADD FULL CYLINDERS              @LV
         ST    R5,AVALCYTM         SAVE UPDATED AVAILABLE CYLS     @LV
         B     F5SKPOLD            SKIP OLD-WAY (CCCCHHHH CODE)    @LV
F5DOOLD  DS    0H                                                  @LV
*******  LH    R5,HOOKHCCH           LOAD END CYLENDER      KBC
         SLR   R5,R5                 CLEAR FOR ICM          KBC
         ICM   R5,B'0011',HOOKHCCH   LOAD END CYLINDER      KBC
         SLR   R0,R0                 CLEAR FOR ICM          KBC
         ICM   R0,B'0011',HOOKLCCH   START CYLINDER         KBC
*******  SH    R5,HOOKLCCH           SUBTRACT START CYL     KBC
         SR    R5,R0                 SUBTRACT START CYL     KBC
         LA    R5,1(R5)              ADJUST COUNT        GDL-94
         CLI   HOOKLCCH+3,X'00'      START ON CYL BOUNDRY GDL-94
         BE    STRTBDY                 YES               GDL-94
         BCTR  R5,0                    NO SUBTRACT 1     GDL-94
STRTBDY  EQU   *
         LH    R0,TRKSPCYL           GET TRACKS PER CYL  GDL-94
         BCTR  R0,0                  BACK UP TO LAST TRK GDL-94
         CH    R0,HOOKHCCH+2         END ON CYL BOUNDRY  GDL-94
         BE    ENDBDY                  YES               GDL-94
         BCTR  R5,0                    NO SUBTRACT 1     GDL-94
ENDBDY   C     R4,F5LARGTT           IS IT THE LARGEST   GDL-94
         BNH   F5AVAIL                 NO, CONTINUE      GDL-94
         ST    R4,F5LARGTT           SAVE NEW LARGEST TR GDL-94
         ST    R5,F5LARGST           NUM CYL IN LARGEST  GDL-94
F5AVAIL  A     R4,AVAILSPC           ADD AVAILABLE TRKS  GDL-94
         ST    R4,AVAILSPC           SAVE NEW AVAIL TRKS GDL-94
         L     R1,AVAILEXT           INCREMENT           GDL-94
         LA    R1,1(R1)                  COUNT OF AVAIL  GDL-94
         ST    R1,AVAILEXT                  EXTENTS      GDL-94
         A     R5,AVAILCYL           ADD FULL CYLENDERS  GDL-94
         ST    R5,AVAILCYL           SAVE NEW AVAIL CYL  GDL-94
F5SKPOLD DS    0H                                                  @LV
         LA    R0,=CL44'  *** FREE EXTENT ***'           GDL-94
         ST    R0,HOOKIDPT                               GDL-94
         MVI   HOOKSEQN,0            FAKE SEQUENCE NUM   GDL-94
         MVI   HOOKTYPE,C'5'         PUT TYPE IN TABLE   GDL-94
         DROP  R2
         OI    FLAGS,NONORMIT        TELL HOOKUP NOT TO NORMALIZE  @LV
*                        BECAUSE, FOR EAV, IT'S ALREADY NORMALIZED @LV
         LA    R2,HOOKPTR
         BAS   R14,HOOKUP
         NI    FLAGS,X'FF'-NONORMIT  RESET FLAG                    @LV
         LM    R2,R5,F5SAVE+8                            GDL-94
         L     R14,F5SAVE+4
         BR    R14
         SPACE 1
*F6HOOK  STC   R5,HOOKSEQN                               GDL-94
*        MVC   HOOKIDPT,=A(FMT6MSG)                      GDL-94
*        LR    R0,R4                                     GDL-94
*        MH    R0,TRKSPCYL                               GDL-94
*        STH   R0,HOOKNTRK                               GDL-94
*        B     F6RETURN                                  GDL-94
         SPACE 1
