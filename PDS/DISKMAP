         BZ    FM1OPTS
         MVI   0(R2),C'R'
         SPACE 1
FM1OPTS  MVI   BUF,SPACE1AP
         TM    PARMOPT1,EMPTYOPT     TEST FOR "EMPTY" OPTION
         BO    FM1EM1         IF REQUEST GO TO PROCESS IT
         TM    PARMOPT2,MODELOPT   TEST FOR "MODEL" OPTION
         BZ    FM1SKTS0   IF NOT MODEL BYPASS..
*
FM1EM1   CLC   DS1LSTAR(5),ZEROES   TEST FOR TTR ZEROED...NOT VALID
         BE    FM1DONXT              IF ALL ZERO...BYPASS
         CLI   DS1LSTAR+2,0         TEST FOR A TTR OF RECORD ZERO
         BNE   FM1DONXT             IF NOT ZERO, NON-EMPTY DSN
         CLC   NUMEXT,ZEROES        TEST FOR ANY VALID EXTENTS
         BNE   FM1EM2              IF NOT ZERO...EMPTY DATASET.
         TM    PARMOPT2,MODELOPT    CHECK FOR "MODEL" OPTION SELECTED
         BO    FM1SKTS0      IF SELECTED...PRINT THIS ENTRY
         B     FM1DONXT    IF NOT MODEL...DON'T INCLUDE IT AS "EMPTY"
FM1EM2   TM    PARMOPT1,EMPTYOPT     CHECK FOR OPTION OF "EMPTY"
         BZ    FM1DONXT  IF NOT SELECTED...DON'T PRINT
*
FM1SKTS0 EQU  *
         TM    PARMOPT1,DUMPOPT
         BO    FM1XTRAL
         TM    PARMOPT1,EXTNTOPT
         BZ    FM1SKTS1
         CLC   NUMEXT,ZEROES
         BNE   FM1XTRAL
*FM1SKTS1 TM    PARMOPT2,ISAMOPT
*         BZ    FM1SKTS2
*         LTR   R7,R7
*         BNZ   FM1XTRAL
FM1SKTS1 DS    0H
FM1SKTS2 TM    PARMOPT1,PDSOPT
         BZ    FM1GOPRT
         TM    DS1DSORG,DCBDSGPO
         BZ    FM1GOPRT
FM1XTRAL MVI   BUF,SPACE2AP
FM1GOPRT TM    PARMOPT1,MAPONOPT
         BO    FM1PRLOP
         TM    PARMOPT2,FREEOPT
         BO    FM1PRLOP
         BAS   R14,PRINT
         SPACE 1
FM1SKTS3 EQU   *
         TM    PARMOPT1,DUMPOPT
         BZ    FM1EXDMP
         L     R8,DS1F3PTR         ADDR OF POSSIBLE F3 (FOR LATER) @LV
         LA    R2,DS1
         LA    R3,DS1FDAD
         BAS   R14,DUMPDSCB
         LTR   R7,R7
         BZ    FM1DMP9             IF NOT ISAM, BRANCH TO CHECK F9 @LV
         LA    R2,DS2
         LA    R3,DS2FDAD
         BAS   R14,DUMPDSCB
FM1DMP9  DS    0H                                                  @LV
         L     R2,DS1F9PTR         ADDR OF POSSIBLE F9             @LV
         LTR   R2,R2               IS THERE AN F9 ?                @LV
         BZ    FM1DMP3             NO, BRANCH TO CHECK FOR AN F3   @LV
         LA    R3,DS9FDAD-DS9(,R2) CCHHR FOR F9 (FOR PRINTING)     @LV
         BAS   R14,DUMPDSCB        GO 'DUMP' (PRINT) THE F9        @LV
FM1DMP3  LTR   R8,R8
         BZ    FM1EXDMP
         LA    R2,DS3
         LA    R3,DS3FDAD
         BAS   R14,DUMPDSCB
*   NOW, DUMP *ALL* FMT-3'S (PREVIOUS CODE JUST DUMPED ONE OF 'EM) @LV
         L     R8,DS3F3PTR         PTR TO NEXT POSSIBLE F3         @LV
         B     FM1DMP3             BRANCH BACK FOR 'NEXT' F3       @LV
         SPACE 1
FM1EXDMP TM    PARMOPT1,EXTNTOPT
         BZ    FM1PDSDP
         CLC   NUMEXT,ZEROES
         BE    FM1PDSDP
         MVC   BUF+10(HEADEDAL),HEADEDAT
         MVI   BUF,SPACE0AP
         BAS   R14,PRINT
         MVC   BUF+10(L'HEAD2UND),HEAD2UND
         MVI   BUF,SPACE1AP
         BAS   R14,PRINT
         L     R2,LISTEXT
         LA    R3,BUF+9
         SR    R4,R4
         MVI   EXPFLAG,X'FF'
         SPACE 1
FM1EXLP  DS    0H                                                KBC
         MVI   BUF+1,C' '           CLEAR ...                    KBC
         MVC   BUF+2(131),BUF+1     PRINT LINE BEFORE FORMATTING KBC
         BAS   R14,EXFORMAT
         LA    R4,4(,R4)
         L     R2,LISTEXT(R4)
         LTR   R2,R2
         BZ    FM1EXLST
         BAS   R14,PRINT
         B     FM1EXLP
FM1EXLST MVI   BUF,SPACE2AP
         BAS   R14,PRINT
         SPACE 1
FM1PDSDP TM    PARMOPT1,PDSOPT
         BZ    FM1ISDMP
         TM    DS1DSORG,DCBDSGPO
         BZ    FM1ISDMP
         TM    DS1SMSFG,DS1PDSEX   IS THIS AN HFS DATASET ?        KBC
         BO    FM1ISDMP            YES, CAN'T OPEN WITH BSAM/QSAM  KBC
         LA    R2,DS1
         BAS   R14,PDSLIST
         SPACE 1
*FM1ISDMP TM    PARMOPT2,ISAMOPT
*         BZ    FM1DONXT
*         BAL   R14,ISAMLIST
FM1ISDMP DS    0H

FM1DONXT B     FM1PRLOP
*
* G.D.F. ROUTINE TO CONVERT INPUT & OUTPUT DATES TO
* SAME FORMAT AS CREDAT & EXPDAT FOR DATEDIT ROUTINE
*
BINARY   UNPK  DASAVE(3),0(2,R2)  G.D.F.
         PACK  DATEWORD,DASAVE(2)  G.D.F.
         CVB   R1,DATEWORD    G.D.F.
         STC   R1,0(R2)       G.D.F.
         ZAP   DATEWORD,1(2,R2)  G.D.F.
         CVB   R1,DATEWORD    G.D.F.
         STH   R1,DATEWORD    G.D.F.
         MVC   1(2,R2),DATEWORD  G.D.F.
         BR    R14            G.D.F.
*
* END OF BINARY CONVERT ROUTINE
*
         SPACE 2
FM1PREND TM    PARMOPT1,MAPONOPT
         BO    TESTMAP
         TM    PARMOPT2,FREEOPT
         BO    TESTMAP
         MVI   BUF,SPACE1IM
         BAS   R14,PRINT
         MVI   BUF+1,C' '          MOVE TO BLANK OUT LINE
         MVC   BUF+2(131),BUF+1    BLANK OUT PRINT LINE
         MVC   BUF+49(14),=C'<<<<< END VTOC'
         LH    R0,FM1COUNT
         BAS   R14,CONVERT
         MVC   BUF+64(4),CONVEDIT+8
         MVC   BUF+69(15),=C'DATA SETS >>>>>'
         MVI   BUF,SPACE3AP
         BAS   R14,PRINT
         SPACE 1
TESTMAP  DS    0H
         TM    PARMOPT2,FREEOPT
         BO    MAPCONT
         TM    PARMOPT1,MAPOPT+DIRFLAG
         BZ    MAPEND
MAPCONT  DS    0H
         SPACE 1
         DROP  R6,R7,R8
         EJECT
MAPPRINT MVC   CURLINES,=H'999'
         LA    R0,HEAD2A
         ST    R0,BUFPTR
         MVI   HEADMID,C' '
         MVC   HEADMID+1(81),HEADMID
         MVC   HEAD2VOL,VOLSERNO
         MVC   HEADMID(40),HEAD2
         MVI   BUF+1,C' '          CLEAR PRINT ...
         MVC   BUF+2(131),BUF+1    BUFFER
         L     R6,HOOKSTRT
         USING EXTENT,R6
         L     R4,=F'-1'
         SR    R5,R5
         MVI   EXPFLAG,0
         SPACE 1
MAPPRLOP CLC   EXTLCCHH,HOOKEND
         BE    MAPPREND
         NI    FLAGS,X'FF'-TRKOVLAP     CLEAR TRACKS-OVERLAP FLAG   KBC
*        CLC   EXTHITT,TRKSPVOL                                    @LVD
*        BNL   MAPINVAL                                            @LVD
*        CLC   EXTLOTT,EXTHITT                                     @LVD
*        BH    MAPINVAL                                            @LVD
         CLI   EXTTYPE,X'80'
         BE    MAPGETEX
*                         R4 CONTAINS PREVIOUS HIGH TRACK  GDL-94
         L     R0,EXTLOTT                                  GDL-94
*        N     R0,=X'0000FFFF'  ZERO OUT HIGH ORDER BYTES  GDL-94
         SR    R0,R4
         S     R0,=F'1'
         BZ    MAPTRGET
         BP    MAPTRMIS
         BAS   R14,CONVERT
         MVC   OVLAPAMT,CONVEDIT+7         SAVE THE OVERLAP AMOUNT KBC
         OI    FLAGS,TRKOVLAP              SET OVERLAP FLAG        KBC
*******  MVC   BUF+1(5),CONVEDIT+7                                 KBCD
*******  MVC   BUF+7(14),=C'TRACKS OVERLAP'                        KBCD
         C     R4,EXTHITT                                  GDL-94
         BNL   *+8
MAPTRGET L     R4,EXTHITT                                  GDL-94
*        N     R4,=X'0000FFFF'  ZERO OUT HIGH ORDER BYTES  GDL-94
         A     R5,EXTNOTRK                                 GDL-94
*        N     R5,=X'0000FFFF'  ZERO OUT HIGH ORDER BYTES  GDL-94
         B     MAPGETEX
         SPACE 1
*MAPINVAL MVC   BUF+7(14),=C'INVALID EXTENT'                       @LVD
*        B     MAPGETEX                                            @LVD
         SPACE 1
MAPTRMIS BAS   R14,CONVERT
         MVC   BUF+1(5),CONVEDIT+7
         MVC   BUF+7(14),=C'TRACKS MISSING'
         MVI   BUF,SPACE1AP
         BAS   R14,PRINT
         B     MAPTRGET
         SPACE 1
MAPGETEX LA    R2,EXTENT
         LA    R3,BUF+1
         MVI   BUF+1,C' '          CLEAR ...                     KBC
         MVC   BUF+2(131),BUF+1    PRINT LINE BEFORE FORMATTING  KBC
         BAS   R14,EXFORMAT
         TM    PARMOPT2,FREEOPT
         BZ    MAPRINTY
         USING EXLINE,R3
         CLC   EXLDSNAM,=CL44'  *** FREE EXTENT ***'
         BNE   MAPSKIPR
         MVC   EXLTYPE,=C'    '
         DROP  R3
MAPRINTY DS    0H
         TM    FLAGS,TRKOVLAP      TRACKS-OVERLAP FLAG SET ?       KBC
         BZ    NOTOVLAP            NO, BRANCH TO PRINT THE LINE    KBC
         MVC   BUF+1(5),OVLAPAMT   OVERLAP AMOUNT TO PRINT LINE    KBC
         MVC   BUF+6(16),=C' TRACKS OVERLAP '  "FLAG" THE LINE     KBC
NOTOVLAP DS    0H                                                  KBC
         BAS   R14,PRINT
MAPSKIPR DS    0H
         L     R6,EXTPTR
         B     MAPPRLOP
         SPACE 3
*                 R4 IS HIGEST TRACK FOUND                 GDL-94
*                 R5 IS TOTAL NUMBER OF TRACKS PROCESSED   GDL-94
MAPPREND L     R0,TRKSPVOL                                 GDL-94
         SR    R0,R5                                       GDL-94
         S     R0,=F'1'
         BNP   MAPNOMIS
         BAS   R14,CONVERT
         MVC   BUF+1(5),CONVEDIT+7
         MVC   BUF+7(40),=C'TRACKS MISSING OR ASSIGNED AS ALTERNATES'
         MVI   BUF,SPACE1AP
         BAS   R14,PRINT
