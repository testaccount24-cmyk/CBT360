VTOCREAD ST    R14,VTSAVE
         STM   R2,R4,VTSAVE+4
         L     R2,RENEXT@
         CHECK (2)
         LA    R1,=CL11'VTOC READ.'
         BAS   R14,SYNADCHK
         SPACE 1
         USING DS1,R3
VTOCREOK XC    0(20,R2),0(R2)
         LA    R4,20(R2)
         MVC   DS1(DS1END-DS1),0(R4)
         MVC   DS1FDAD(3),VTOCMBB
         MVC   DS1FDAD+3(4),VTOCCCHH
         MVC   DS1FDAD+7(1),VTOCR
         CLC   HADSCB,=X'0000000000'   IS HADSCB ZERO ? (OSVTOC)  @LV2A
         BE    TSTHWMDS                YES, GO TEST HWM (DS4HPCHR)@LV2A
         CLC   DS1FDAD+3(5),HADSCB     IXVTOC HWM BEEN REACHED ?  @LV2A
         BH    VTOCEOD                 YES, DON'T READ MORE DSCBS @LV2A
         B     VTOCRD01                NO, GO READ NEXT DSCB      @LV2A
TSTHWMDS DS    0H                                                 @LV2A
         CLC   HWMDS1,DS1FDAD+3
         BNL   VTOCRD01                         ADDED 12/11/81 KOWNACKI
         CLC   HWMDS1,=X'0000000000'        ALLOW INITIAL X'00' AS
         BNE   VTOCEOD                      DS4HPCHR FOR DSF
         SPACE 1
VTOCRD01 DS    0H
         L     R15,CHGTO24B            LOAD ADDR OF 'READ24' WITH
         BSM   0,R15                   CHANGE TO 24-BIT MODE IN ORDER
*                                      TO DO I/O
CHGTO24B DC    0F'0',AL1(00),AL3(READ24)   SWITCH TO 24-BIT TO DO I/O
READ24   DS    0H
         READ  (2),SF,VTOCDCB,(4),MF=E
         L     R15,CHGTO31C             LOAD ADDR OF 'READ31' WITH
         BSM   0,R15                   CHANGE TO 31-BIT MODE IN ORDER
*                                      TO ADDRESS GETCORE STORAGE
CHGTO31C DC    0F'0',A(X'80000000'+READ31)   SWITCH BACK TO 31-BIT
READ31   DS    0H
         A     R2,REBLKSIZ
         ST    R2,RENEXT@
         L     R1,RECOUNT
         BCT   R1,VTSAVECT
         L     R2,REBLOCK@
         ST    R2,RENEXT@
         L     R1,RENCP
VTSAVECT ST    R1,RECOUNT
         SPACE 1
         LA    R0,1
         SR    R1,R1
         IC    R1,VTOCR
         AR    R1,R0
         STC   R1,VTOCR
         CH    R1,VTDSCBTR
         BNH   VTOCREND
         MVI   VTOCR,1
         SLR   R15,R15                                            @LV2
         ICM   R15,B'0011',VTOCTT                                 @LV2C
         AR    R15,R0                                             @LV2
         STCM  R15,B'0011',VTOCTT                                 @LV2C
         LR    R1,R15                                             @LV2C
         BAS   R14,TTCNVOLD                                       @LV2C
         MVC   VTOCCCHH,CCHH
         SPACE 1
VTOCREND L     R14,VTSAVE
         LM    R2,R4,VTSAVE+4
         LA    R14,4(R14)
         BR    R14
         SPACE 1
VTOCEOD  L     R14,VTSAVE
         LM    R2,R4,VTSAVE+4
         BR    R14
         SPACE 1
         DROP  R3
         EJECT
         SPACE 3
OBTAIN   MVC   OBCCHHR,0(R2)
         OBTAIN OBCAMLST,EADSCB=OK                                 @LV
         BXLE  R15,R15,*+8
         BAS   R14,ABDUMP
         USING DS1,R3
         MVC   DS1(DS1END-DS1),CAMLIST
         MVC   DS1FDAD(3),VTOCMBB
         MVC   DS1FDAD+3(5),0(R2)
         BR    R14
         SPACE 2
